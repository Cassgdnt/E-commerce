{% extends 'base.html.twig' %}

{% block title %}Profil{% endblock %}

{% block body %}

<div class="Profil">
    <h2>{{'Profil.Nom'|trans}} {{user.nom}}</h2>
    <h2>{{'Profil.Prenom'|trans}} {{user.prenom}}</h2>
    <p>{{'Profil.Email'|trans}} {{user.email}}</p>
</div>
<a href="{{path('editProfil', {id:app.user.id})}}" class="btn btn-primary">Modifier mes informations</a>

{% endblock %}
